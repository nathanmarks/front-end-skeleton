  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --progress --colors",
    "build:babel": "cross-env NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./build",
    "clean": "rimraf build && rimraf static",
    "lint": "eslint src test && echo \"eslint: no lint errors\"",
    "start": "node webpack.dev.server.js",
    "test": "cross-env NODE_ENV=test babel-node test/index.js",
    "test:coverage": "cross-env NODE_ENV=test babel-node ./node_modules/istanbul/lib/cli.js cover test/index.js",
    "test:watch": "cross-env NODE_ENV=test babel-node test/watch.js"
  },
  "dependencies": {
    "dom-helpers": "^2.4.0",
    "normalize.css": "^4.1.1"
  },
  "devDependencies": {
    "app-module-path": "^1.1.0",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-mocha": "^3.0.0",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.5",
    "html-webpack-plugin": "^2.21.0",
    "istanbul": "^1.1.0-alpha.1",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
